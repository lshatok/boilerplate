/* SYSTEM MONITORING STYLES */
/* EDIT THESE STYLES TO CHANGE THE BRANDING COLORS */
 .topBanner {

background: #032b3b; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(left,  #032b3b 0%, #ffffff 65%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, right top, color-stop(0%,#032b3b), color-stop(65%,#ffffff)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(left,  #032b3b 0%,#ffffff 65%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(left,  #032b3b 0%,#ffffff 65%); /* Opera 11.10+ */
background: -ms-linear-gradient(left,  #032b3b 0%,#ffffff 65%); /* IE10+ */
background: linear-gradient(to right,  #032b3b 0%,#ffffff 65%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#032b3b', endColorstr='#ffffff',GradientType=1 ); /* IE6-8 */

}

/* END TOP BANNER */

